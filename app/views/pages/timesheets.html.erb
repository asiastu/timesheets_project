<div class="dashboard-container">

  <h1>Timesheets</h1>

  <%= link_to :back do %>
      <i class="fas fa-arrow-left profile-back", title="back"></i>
    <% end %>

  <div class="row">

    <% unless @timesheets == [] %>
      <% @timesheets.first.each do |timesheet| %>
        <div class="col-xs-12">
          <div class ="dashboard-card">

        <%= link_to "FROM: #{timesheet.week_start.strftime('%A, %d %B %Y')} - TO: #{timesheet.week_end.strftime('%A, %d %B %Y')}", apprentice_placement_timesheet_path(timesheet.placement.apprentice, timesheet.placement, timesheet) %>
      </div>
      </div>
      <% end %>
     <% end %>

  </div>

</div>

